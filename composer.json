{
    "name": "vasileuski/markdown",
    "description": "Markdown Builder",
    "keywords": [
        "markdown",
        "template",
        "builder",
        "render"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dzmitry Vasileuski",
            "email": "vasileuski_dzmitry@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.*",
        "squizlabs/php_codesniffer": "3.4.*"
    },
    "autoload": {
        "psr-4": {
            "Vasileuski\\Markdown\\": "src",
            "Vasileuski\\MarkdownTests\\": "tests"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
